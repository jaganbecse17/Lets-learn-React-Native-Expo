{
    "importOrder": [
        "^react",
        "<BUILTIN_MODULES>",
        "<THIRD_PARTY_MODULES>",
        "^@ui/(.*)$",
        "^@hooks/(.*)$",
        "^@contexts/(.*)$",
        "^@constants/(.*)$",
        "^~/(.*)$",
        "^[./]",
        "^@components/(.*)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all"
}
